<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.AmbangLebarPengontrolSegiempatActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_alps"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:text="Ambang Lebar Pengontrol Segiempat" />

            <com.levitnudi.legacytableview.LegacyTableView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/alps_table"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_alps" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mencari Nilai Cd"
                android:id="@+id/tv_cd"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toBottomOf="@+id/alps_table"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_cd"
                app:layout_constraintLeft_toLeftOf="parent"
                android:id="@+id/tv_q"
                android:text="Rumus Cd ="
                android:textSize="14sp"
                android:textColor="@color/black" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="55dp"
                app:layout_constraintLeft_toRightOf="@+id/tv_q"
                android:id="@+id/iv_rumus"
                android:layout_marginRight="10dp"
                android:scaleType="fitXY"
                android:src="@drawable/alps_rumus_cd"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_q"
                app:layout_constraintBottom_toBottomOf="@+id/tv_q"/>

            <com.levitnudi.legacytableview.LegacyTableView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/alps_cd"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/iv_rumus" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mencari Nilai Cv"
                android:id="@+id/tv_cv"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toBottomOf="@+id/alps_cd"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_cv"
                app:layout_constraintLeft_toLeftOf="parent"
                android:id="@+id/tv_cd_rumus"
                android:text="Rumus Cv ="
                android:textSize="14sp"
                android:textColor="@color/black" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="55dp"
                app:layout_constraintLeft_toRightOf="@+id/tv_cd_rumus"
                android:id="@+id/iv_rumus_cv"
                android:layout_marginRight="10dp"
                android:scaleType="fitXY"
                android:src="@drawable/alps_rumus_cv"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_cd_rumus"
                app:layout_constraintBottom_toBottomOf="@+id/tv_cd_rumus"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/iv_rumus_cv"
                app:layout_constraintLeft_toLeftOf="parent"
                android:id="@+id/tv_cd_bentuk"
                android:text="Bentuk ="
                android:textSize="14sp"
                android:textColor="@color/black" />

            <com.levitnudi.legacytableview.LegacyTableView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/alps_cv"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_cd_bentuk" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Debit"
                android:id="@+id/tv_debit"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toBottomOf="@+id/alps_cv"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_debit"
                app:layout_constraintLeft_toLeftOf="parent"
                android:id="@+id/tv_rumus_debit"
                android:text="Rumus Debit ="
                android:textSize="14sp"
                android:textColor="@color/black" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="55dp"
                app:layout_constraintLeft_toRightOf="@+id/tv_rumus_debit"
                android:id="@+id/iv_rumus_debit"
                android:layout_marginRight="10dp"
                android:scaleType="fitXY"
                android:src="@drawable/alps_rumus_debit"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_rumus_debit"
                app:layout_constraintBottom_toBottomOf="@+id/tv_rumus_debit"/>

            <com.levitnudi.legacytableview.LegacyTableView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/alps_debit"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/iv_rumus_debit" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>